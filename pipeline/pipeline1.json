{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "fake_customers CSV to SQL",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "Names",
									"type": "String",
									"physicalType": "nvarchar"
								},
								"sink": {
									"name": "Names",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "Age",
									"type": "Double",
									"physicalType": "float"
								},
								"sink": {
									"name": "Age",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "Phone",
									"type": "String",
									"physicalType": "nvarchar"
								},
								"sink": {
									"name": "Phone",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "Location",
									"type": "String",
									"physicalType": "nvarchar"
								},
								"sink": {
									"name": "Location",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "Company",
									"type": "String",
									"physicalType": "nvarchar"
								},
								"sink": {
									"name": "Company",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "Lot",
									"type": "String",
									"physicalType": "nvarchar"
								},
								"sink": {
									"name": "Lot",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "Sales",
									"type": "Byte",
									"physicalType": "tinyint"
								},
								"sink": {
									"name": "Sales",
									"type": "String",
									"physicalType": "String"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "AzureSqlTable1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}